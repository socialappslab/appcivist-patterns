<main class="appmain">

	{{> molecules-navigation}}

	<article class="working_group">

		<header class="page__header">
		  <div class="type">Working Group</div>

			<div class="title_block">
			  <h1 class="heading--title">Quality of Life</h1>
				<div class="heading_actions">
	        <ul>
	          <li class="action__contextual_menu"><a href="#">{{> atoms-icon-cog }}</a></li>
	        </ul>
					<div class="contextual_menu">
			      <div class="content">
							<ul class="actions">
			          <li><a href="#">Edit</a></li>
			          <li><a href="#">Publish</a></li>
			          <li><a href="#">Give Feedback</a></li>
			          <li><a href="#">Exclude</a></li>
			          <li><a href="#">See history</a></li>
			        </ul>
			      </div>
			    </div>
	      </div>
			</div>

			<p class="description">This is the working group dedicated to looking out for the general wellbeing of our citizens.</p>

		</header>

		<div class="page__contents">
			<section class="panel activity-and-members">
				<section class="latest-activity">
					<div class="panel-heading panel-heading-relative">
						<h1 class="heading--header">Latest Activity</h1><p class="heading--small"><a href="{{ link.templates-all-proposals-view }}">See All</a></p>
					</div>
					{{# listItems.ten }}
					{{> atoms-activity-stream-list-item }}
					{{/ listItems.ten }}
				</section>
				<section>
					<div class="panel-heading">
						<h1 class="heading--header">Members</h1><p class="heading--small"><a href="{{ link.templates-all-proposals-view }}">See All</a> | <a href="#">Add New</a></p>
					</div>
					{{# listItems.nine }}
					{{> molecules-user-card }}
					{{/ listItems.nine }}
				</section>
			</section>
		</div>

		{{> organisms-proposals-and-ideas}}

		{{> organisms-comment-section}}

	</article>
</main>
{{> organisms-footer }}
